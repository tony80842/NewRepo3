@model IEnumerable<GGFPortal.DataSetSource.GGF多語對照表>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.程式)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.資料代號)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.中文)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.英文)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.越文)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.說明)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.建立時間)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.異動時間)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.程式)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.資料代號)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.中文)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.英文)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.越文)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.說明)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.建立時間)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.異動時間)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
                @Html.ActionLink("Details", "Details", new { id=item.id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.id })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
